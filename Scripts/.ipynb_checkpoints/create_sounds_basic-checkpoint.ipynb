   "source": [
    "from pydub import AudioSegment\n",
    "\n",
    "\n",
    "cgb = AudioSegment.from_wav(\"../../cgb.wav\")\n",
    "\n",
    "cgb = cgb[:1000]\n",
    "\n",
    "wholes = cgb * 30\n",
    "\n",
    "cgb = cgb[:500]\n",
    "\n",
    "halves = cgb * 60\n",
    "\n",
    "wholes.export('../../wholes.wav', format = 'wav')\n",
    "halves.export('../../havles.wav', format = 'wav')\n"
   ]
  }
